{
  "description": "Compiles web components to separate files for javascript, styles and template markup.",
  "names": [
    "trucks"
  ],
  "type": "program",
  "name": "trucks",
  "summary": "compiles web components",
  "synopsis": [
    "[flags] [options] [files...]"
  ],
  "options": {
    "out": {
      "key": "out",
      "description": "Output directory",
      "names": [
        "-o",
        "--out"
      ],
      "type": "option",
      "extra": " [dir]",
      "required": false,
      "multiple": false,
      "value": "cwd",
      "name": "--out"
    },
    "name": {
      "key": "name",
      "description": "Name for output files",
      "names": [
        "-n",
        "--name"
      ],
      "type": "option",
      "extra": " [val]",
      "required": false,
      "multiple": false,
      "value": "components",
      "name": "--name"
    },
    "html": {
      "key": "html",
      "description": "Template output file",
      "names": [
        "-m",
        "--html"
      ],
      "type": "option",
      "extra": " [file]",
      "required": false,
      "multiple": false,
      "name": "--html"
    },
    "css": {
      "key": "css",
      "description": "Stylesheet output file",
      "names": [
        "-s",
        "--css"
      ],
      "type": "option",
      "extra": " [file]",
      "required": false,
      "multiple": false,
      "name": "--css"
    },
    "js": {
      "key": "js",
      "description": "Javascript output file",
      "names": [
        "-j",
        "--js"
      ],
      "type": "option",
      "extra": " [file]",
      "required": false,
      "multiple": false,
      "name": "--js"
    },
    "transforms": {
      "key": "transforms",
      "description": "Run transform plugins",
      "names": [
        "-t",
        "--transforms"
      ],
      "type": "option",
      "extra": " [name...]",
      "required": false,
      "multiple": true,
      "name": "--transforms"
    },
    "rc": {
      "key": "rc",
      "description": "Configuration files to require",
      "names": [
        "-c",
        "--rc"
      ],
      "type": "option",
      "extra": " [file...]",
      "required": false,
      "multiple": true,
      "name": "--rc"
    },
    "extract": {
      "key": "extract",
      "description": "Extract shadow styles to directory",
      "names": [
        "-e",
        "--extract"
      ],
      "type": "option",
      "extra": " [dir]",
      "required": false,
      "multiple": false,
      "name": "--extract"
    },
    "inject": {
      "key": "inject",
      "description": "Inject shadow styles from directory",
      "names": [
        "-i",
        "--inject"
      ],
      "type": "option",
      "extra": " [dir]",
      "required": false,
      "multiple": false,
      "name": "--inject"
    },
    "manifest": {
      "key": "manifest",
      "description": "Write manifest to file",
      "names": [
        "-a",
        "--manifest"
      ],
      "type": "option",
      "extra": " [file]",
      "required": false,
      "multiple": false,
      "name": "--manifest"
    },
    "force": {
      "key": "force",
      "description": "Force overwrite existing files",
      "names": [
        "-f",
        "--force"
      ],
      "type": "flag",
      "name": "--force"
    },
    "printImports": {
      "key": "printImports",
      "description": "Print the file hierarchy",
      "names": [
        "--print-imports"
      ],
      "type": "flag",
      "name": "--print-imports"
    },
    "printTree": {
      "key": "printTree",
      "description": "Print the component tree hierarchy",
      "names": [
        "--print-tree"
      ],
      "type": "flag",
      "name": "--print-tree"
    },
    "printManifest": {
      "key": "printManifest",
      "description": "Print the generated files manifest",
      "names": [
        "--print-manifest"
      ],
      "type": "flag",
      "name": "--print-manifest"
    },
    "help": {
      "key": "help",
      "description": "Display help and exit",
      "names": [
        "-h",
        "--help"
      ],
      "type": "flag",
      "name": "--help"
    },
    "version": {
      "key": "version",
      "description": "Print the version and exit",
      "names": [
        "--version"
      ],
      "type": "flag",
      "name": "--version"
    }
  },
  "zsh": [
    {
      "info": "zsh",
      "literal": ":file:_files -g '*.html'"
    }
  ]
}
