{
  "description": "Compiles web components to separate files for javascript, styles and template markup.",
  "names": [
    "trucks"
  ],
  "type": "program",
  "name": "trucks",
  "summary": "compiles web components",
  "synopsis": [
    "[flags] [options] [files...]"
  ],
  "options": {
    "html": {
      "key": "html",
      "description": "Template output file.",
      "names": [
        "-m",
        "--html"
      ],
      "type": "option",
      "extra": " [file]",
      "required": false,
      "multiple": false,
      "name": "--html"
    },
    "css": {
      "key": "css",
      "description": "Stylesheet output file.",
      "names": [
        "-s",
        "--css"
      ],
      "type": "option",
      "extra": " [file]",
      "required": false,
      "multiple": false,
      "name": "--css"
    },
    "js": {
      "key": "js",
      "description": "Javascript output file.",
      "names": [
        "-j",
        "--js"
      ],
      "type": "option",
      "extra": " [file]",
      "required": false,
      "multiple": false,
      "name": "--js"
    },
    "help": {
      "key": "help",
      "description": "Display help and exit",
      "names": [
        "-h",
        "--help"
      ],
      "type": "flag",
      "name": "--help"
    },
    "version": {
      "key": "version",
      "description": "Print the version and exit",
      "names": [
        "--version"
      ],
      "type": "flag",
      "name": "--version"
    }
  },
  "zsh": [
    {
      "info": "zsh",
      "literal": ":file:_files -g '*.html'"
    }
  ]
}
